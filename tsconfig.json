{
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "module": "commonjs",
    // "moduleResolution": "nodenext",
    "rootDirs": [
      "./src"
    ],
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}